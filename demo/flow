#!/usr/local/bin/yoda from

: foo1 if then ;              "if then"            cr type$ cr see foo1
: foo2 if else then ;         "if else then"       cr type$ cr see foo2
: foo3 begin again ;          "begin again"        cr type$ cr see foo3
: foo4 begin until ;          "begin until"        cr type$ cr see foo4
: foo5 begin while repeat ;   "begin while repeat" cr type$ cr see foo5
: foo6 for next ;             "for next"           cr type$ cr see foo6
: foo7 do loop  ;             "do loop"            cr type$ cr see foo7
\ : foo8 do +loop  ;            "do +loop"           cr type$ cr see foo8
