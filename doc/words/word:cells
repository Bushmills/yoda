name:                cells
stack:               ( n1 -- n2 )
return stack:        ( -- )
string stack:        ( -- )
contexts:            interpreting, compiling
category:            arithmetics, system


context:             interpreting
description:         returns as n2 the count of cells needed to store n1 integers


context:             compiling
description:         effectively a no ooperation, not compiling any code,
                     therefore leaving n1 on stack unmodified

standard:            yes
url:                 https://forth-standard.org/standard/core/CELLS
